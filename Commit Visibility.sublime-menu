[
    {
        "caption": "隐藏所有该提交的分支",
        "command": "hide_all_commit_refs",
        "args": {"commit": "$commit"}
    },
    {
        "caption": "隐藏除该提交之外的所有分支",
        "command": "hide_all_refs_except_refs_at_commit",
        "args": {"commit": "$commit"}
    },
    {
        "caption": "显示所有隐藏的分支",
        "command": "show_all_refs"
    },
]